{
  "name": "crew-schedule",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "clean": "del-cli pnpm-lock.yaml && del-cli ./node_modules && pnpm i",
    "db:reset": "del-cli ./prisma/migrations && prisma migrate reset --force && prisma db push && tsx ./prisma/seed.ts",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --plugin-search-dir . --write .",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "test": "npm run test:integration && npm run test:unit",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "preview": "vite preview"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.5.1",
    "@prisma/client": "5.2.0",
    "@supabase/auth-helpers-sveltekit": "^0.10.2",
    "@supabase/supabase-js": "^2.33.1",
    "async-retry": "^1.3.3",
    "dayjs": "^1.11.9",
    "dotenv-flow": "^3.3.0",
    "leaflet": "^1.9.4",
    "pg": "^8.11.3",
    "pino": "^8.15.0",
    "svelte-material-icons": "^3.0.5",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^5.0.8",
    "@neoconfetti/svelte": "^1.0.0",
    "@playwright/test": "^1.37.1",
    "@skeletonlabs/skeleton": "^2.0.0",
    "@skeletonlabs/tw-plugin": "^0.1.0",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.24.0",
    "@tailwindcss/forms": "^0.5.6",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/async-retry": "^1.4.5",
    "@types/cookie": "^0.5.1",
    "@types/leaflet": "^1.9.4",
    "@types/node": "^20.5.9",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitest/ui": "^0.34.3",
    "autoprefixer": "^10.4.15",
    "cross-env": "^7.0.3",
    "del-cli": "^5.1.0",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte": "^2.33.0",
    "npm-check-updates": "^16.13.2",
    "postcss": "^8.4.29",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "prisma": "^5.2.0",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "tsx": "^3.12.8",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-tailwind-purgecss": "^0.1.3",
    "vitest": "^0.34.3"
  }
}
