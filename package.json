{
  "name": "crew-schedule",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "db:reset": "cross-env NODE_ENV=development knex migrate:down && npx cross-env NODE_ENV=development knex migrate:up && npx cross-env NODE_ENV=development knex seed:run",
    "db:prisma": "npx prisma migrate reset && npx prisma generate",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --plugin-search-dir . --write .",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "test": "npm run test:integration && npm run test:unit",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "preview": "vite preview"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.5.1",
    "@prisma/client": "5.2.0",
    "@supabase/auth-helpers-sveltekit": "^0.10.2",
    "@supabase/supabase-js": "^2.33.1",
    "async-retry": "^1.3.3",
    "dotenv-flow": "^3.2.0",
    "leaflet": "^1.9.4",
    "pg": "^8.11.3",
    "pino": "^8.15.0",
    "svelte-material-icons": "^3.0.5",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^5.0.8",
    "@neoconfetti/svelte": "^1.0.0",
    "@playwright/test": "^1.37.1",
    "@skeletonlabs/skeleton": "^1.12.0",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.23.0",
    "@tailwindcss/forms": "^0.5.5",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/async-retry": "^1.4.5",
    "@types/cookie": "^0.5.1",
    "@types/leaflet": "^1.9.3",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitest/ui": "^0.34.2",
    "autoprefixer": "^10.4.15",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte": "^2.33.0",
    "npm-check-updates": "^16.13.1",
    "postcss": "^8.4.28",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.2",
    "prettier-plugin-svelte": "^3.0.3",
    "prisma": "^5.2.0",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
