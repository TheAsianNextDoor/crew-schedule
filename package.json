    "@vercel/postgres": "^0.4.0",
    "async-retry": "^1.3.3",
    "dotenv-flow": "^3.2.0",
    "knex": "^2.5.1",
    "leaflet": "^1.9.4",
    "pg": "^8.11.1",
    "pino": "^8.14.1",
    "svelte-material-icons": "^3.0.5",
    "tailwind-merge": "^1.14.0",
    "tsx": "^3.12.7",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^5.0.5",
    "@neoconfetti/svelte": "^1.0.0",
