{
  "name": "crew-schedule",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check-deps": "npm-check-updates",
    "clean": "del-cli pnpm-lock.yaml && del-cli ./node_modules && pnpm i",
    "db:reset": "del-cli ./prisma/migrations && prisma migrate reset --force && prisma db push && tsx ./prisma/seed.ts && prisma generate",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --plugin-search-dir . --write .",
    "lint": "prettier --plugin-search-dir . --write . && eslint --fix .",
    "test": "pnpm test:integration && pnpm test:unit",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "preview": "vite preview"
  },
  "dependencies": {
    "@easepick/core": "^1.2.1",
    "@easepick/datetime": "^1.2.1",
    "@easepick/range-plugin": "^1.2.1",
    "@floating-ui/dom": "^1.5.3",
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@googlemaps/js-api-loader": "^1.16.2",
    "@prisma/client": "5.5.2",
    "@skeletonlabs/skeleton": "^2.4.0",
    "@supabase/auth-helpers-sveltekit": "^0.10.3",
    "@supabase/supabase-js": "^2.38.4",
    "@sveltejs/adapter-vercel": "^3.0.3",
    "async-retry": "^1.3.3",
    "dayjs": "^1.11.10",
    "pg": "^8.11.3",
    "pino": "^8.16.1",
    "svelte-dnd-action": "^0.9.31",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^5.0.8",
    "@playwright/test": "^1.39.0",
    "@skeletonlabs/tw-plugin": "^0.2.3",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.27.1",
    "@tailwindcss/forms": "^0.5.6",
    "@tanstack/svelte-query": "^5.3.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@types/async-retry": "^1.4.7",
    "@types/cookie": "^0.5.3",
    "@types/google.maps": "^3.54.5",
    "@types/node": "^20.8.9",
    "@types/pg": "^8.10.7",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "del-cli": "^5.1.0",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-svelte": "^2.34.0",
    "npm-check-updates": "^16.14.6",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "prisma": "^5.5.2",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.2",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-tailwind-purgecss": "^0.1.3",
    "vitest": "^0.34.6"
  }
}
